struct node {
    int data;
    struct node* next;
};

typedef struct node* NODE;

NODE createNode(int x) {
    //write your code here..
    NODE newNode;

	newNode = (NODE)malloc(sizeof(struct node));
	if(newNode == NULL) {
		printf("Memory allocation failed\n");
		return NULL;
	}
    
    newNode->data = x;
	newNode->next = NULL;

	return newNode;
    
}

NODE addNode(NODE first, int x) {
    //write your code here..
    NODE newNode = createNode(x);

    if (newNode == NULL)
        return first;

    /* If list is empty */
    if (first == NULL) {
        return newNode;
    }

    /* Traverse to last node */
    NODE temp = first;
    while (temp->next != NULL) {
        temp = temp->next;
    }

    temp->next = newNode;

    return first;

    
}

NODE concatenate(NODE t1, NODE t2) {
    //write your code here..
    if (t1 == NULL)
        return t2;

    /* If second list is empty */
    if (t2 == NULL)
        return t1;

    NODE temp = t1;

    /* Move to last node of first list */
    while (temp->next != NULL) {
        temp = temp->next;
    }

    /* Link last node of first list to head of second list */
    temp->next = t2;

    return t1;

    
    
    
}

void displayList(NODE first) {
    //write your code here..
    NODE temp = first;

    while (temp != NULL) {
        printf("%d --> ", temp->data);
        temp = temp->next;
    }

    printf("NULL\n");

    
    
    
}
